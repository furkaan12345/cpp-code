#include <iostream>
using namespace std;


int main() {
    int a, b;

    cout << "Enter value for a: ";
    cin >> a;
    cout << "Enter value for b: ";
    cin >> b;

    int temp = a;
    a = b;
    b = temp;

    std::cout << "After swapping, a = " << a << " and b = " << b << std::endl;

    return 0;
}
